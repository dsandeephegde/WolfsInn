<p id="notice"><%= notice %></p>

<h1>Checkins</h1>

<table>
  <thead>
    <tr>
      <th>Startdate</th>
      <th>Enddate</th>
      <th>Checkintime</th>
      <th>Checkouttime</th>
      <th>Numberofguests</th>
      <th>Total</th>
      <th>Customerid</th>
      <th>Hotelid</th>
      <th>Roomnumber</th>
      <th>Paymentid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @checkins.each do |checkin| %>
      <tr>
        <td><%= checkin.startDate %></td>
        <td><%= checkin.endDate %></td>
        <td><%= checkin.checkinTime %></td>
        <td><%= checkin.checkoutTime %></td>
        <td><%= checkin.numberOfGuests %></td>
        <td><%= checkin.total %></td>
        <td><%= checkin.customerId %></td>
        <td><%= checkin.hotelId %></td>
        <td><%= checkin.roomNumber %></td>
        <td><%= checkin.paymentId %></td>
        <td><%= link_to 'Show', checkin %></td>
        <td><%= link_to 'Edit', edit_checkin_path(checkin) %></td>
        <td><%= link_to 'Destroy', checkin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Checkin', new_checkin_path %>
